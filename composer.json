{
    "name": "swop/github-webhook-security-checker",
    "license": "MIT",
    "type": "library",
    "description": "Security checker which will verify if the incoming GitHub web hook request is correctly signed.",
    "keywords": [
        "github",
        "webhook",
        "security"
    ],
    "authors": [
        {
            "name": "Sylvain Mauduit",
            "email": "sylvain@mauduit.fr",
            "homepage": "https://github.com/Swop"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5.0",
        "zendframework/zend-diactoros": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Swop\\GitHub\\WebHookSecurityChecker\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
