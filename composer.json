{
    "name": "swop/github-webhook",
    "license": "MIT",
    "type": "library",
    "description": "Library which deals with incoming GitHub web hooks requests (signature validation & payload parsing)",
    "keywords": [
        "github",
        "webhook",
        "security"
    ],
    "authors": [
        {
            "name": "Sylvain Mauduit",
            "email": "sylvain@mauduit.fr",
            "homepage": "https://github.com/Swop"
        }
    ],
    "require": {
        "php": ">=5.4",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5.0",
        "zendframework/zend-diactoros": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Swop\\GitHubWebHook\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
